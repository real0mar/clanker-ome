{
    "compilerOptions":  {
                            "target":  "ES2021",
                            "module":  "CommonJS",
                            "moduleResolution":  "Node",
                            "lib":  [
                                        "ES2021",
                                        "DOM"
                                    ],
                            "strict":  true,
                            "esModuleInterop":  true,
                            "forceConsistentCasingInFileNames":  true,
                            "skipLibCheck":  true
                        },
    "include":  [
                    "api/**/*.ts"
                ],
    "exclude":  [
                    "node_modules"
                ]
}
